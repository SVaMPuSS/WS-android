<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <rotate
            android:fromDegrees="-45"
            android:pivotX="0%"
            android:pivotY="0%"
            android:toDegrees="0">
            <shape>
                <solid android:color="#1C1C1C" />
            </shape>
        </rotate>
    </item>

    <item android:left="15dp">
        <shape>
            <solid android:color="#1C1C1C" />
        </shape>
    </item>


</layer-list>